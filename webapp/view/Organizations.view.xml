<core:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" controllerName="myhealthapp.controller.Patients"
	xmlns:html="http://www.w3.org/1999/xhtml">
	<Page title="{i18n>title}" showNavButton="true" navButtonPress="onNavBack">
		<content>
			<List id="organizationsList" headerText="{i18n>organizationsTitle}" items="{/Organization}">
                <StandardListItem title="{name}"/>
            </List>
		</content>
		<footer>
			<Toolbar>
				<Label text="{i18n>homeFooterText}"/>
			</Toolbar>
		</footer>
	</Page>
</core:View>